.section text
    .temps r6, r7
    .zero r0

.section init
    r0 := 0

.section text
    print_stack:
        push r1 on stack r2
        push r4 on stack r2
        push r3 on stack r2

        output ">>> "

        r5 := m[r0][r2 + 3]
        if (r5 == 0) goto print_zero

        if (r5 <s 0) goto print_neg using r1

        push r5 on stack r2
        goto print_pos linking r1
        
        goto finish_print_stack

    print_zero:
        r5 := 48
        output r5

        push r5 on stack r2
        goto finish_print_stack

    print_neg:
        r5 := r5 * -1
        output 45
        push r5 on stack r2
        goto print_pos linking r1
        goto finish_print_stack

    print_pos:
        push r1 on stack r2
        push r4 on stack r2
        push r3 on stack r2
        
        r3 := m[r0][r2 + 3]                 # r3 stores the current value

        r4 := r3 / 10
        push r3 on stack r2
        if (r4 == 0) goto base_case_print using r5 
        pop stack r2

        push r4 on stack r2
        goto print_pos linking r1
        pop stack r2

        r4 := r3 mod 10                     
        r4 := r4 + 48
        output r4
        
        goto finish_print_pos

    base_case_print:
        pop r3 off stack r2
        r4 := r3 mod 10
        r4 := r4 + 48
        output r4

        goto finish_print_pos

    finish_print_pos:
        pop r3 off stack r2
        pop r4 off stack r2
        pop r5 off stack r2
        goto r5

    finish_print_stack:
        pop stack r2

        pop r3 off stack r2
        pop r4 off stack r2
        pop r5 off stack r2
        goto r5